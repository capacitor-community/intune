{
  "name": "@ionic-enterprise/intune",
  "version": "3.1.0",
  "description": "Support for Microsoft Intune",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "cordova": {
    "id": "@ionic-enterprise/intune",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "files": [
    "android",
    "cordova",
    "dist/",
    "scripts",
    "ios",
    "plugin.xml",
    "IonicEnterpriseIntune.podspec"
  ],
  "author": "",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/enterprise-intune.git"
  },
  "bugs": {
    "url": "https://github.com/ionic-team/enterprise-intune/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- autocorrect --format",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api IntunePlugin --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "node ../../scripts/update-docs-changelog.js && pnpm run build"
  },
  "devDependencies": {
    "@capacitor/android": ">=4.0.0",
    "@capacitor/core": ">=4.0.0",
    "@capacitor/docgen": "^0.0.10",
    "@capacitor/ios": ">=4.0.0",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "^1.0.1",
    "@ionic/swiftlint-config": "^1.1.2",
    "eslint": "^7.11.0",
    "prettier": "~2.2.0",
    "prettier-plugin-java": "~1.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.0",
    "swiftlint": "^1.0.1",
    "typescript": "~4.2.0"
  },
  "peerDependencies": {
    "@capacitor/core": ">=4.0.0"
  },
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  }
}
